doctype
html(lang="de",class="wf-loading")
  head
    link(href="main.css",rel="stylesheet",type="text/css")
    title= title
    meta(name="description",content="#{ description }")
    meta(charset="utf-8")
    meta(http-equiv="X-UA-Compatible",content="IE=edge")
    meta(name="viewport",content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no, minimal-ui")
    meta(name="mobile-web-app-capable",content="yes")
    meta(name="theme-color",content="#00519e")
    meta(name="copyright",content="All contents copyright 2015 kompeDent Zahntechnik GmbH")
    meta(name="keywords",content="Zahn, Zahntechnik, ")
    meta(name="author",content="kompeDent Zahntechnik GmbH, Neuenstadt Germany - 2015 / www.kompedent.de")
    meta(name="audience",content="Alle")
    meta(name="expires",content="NEVER")
    meta(name="description",content="")
    meta(name="revisit-after",content="3 days")
    meta(http-equiv="imagetoolbar",content="no")
    meta(name="robots",content="INDEX,FOLLOW")
    meta(property="og:locale",content="de_DE")
    meta(property="og:site_name",content="kompeDent Zahntechnik, Neuenstadt")
    meta(property="og:title",content="Mehr halten als versprechen.")
    meta(property="og:url",content="http://www.kompedent.de/")
    meta(property="og:type",content="article")
    meta(property="og:description",content="kompeDent Zahntechnik, Neuenstadt")
    meta(property="og:image",content="http://www.kompedent.de/public/images/logo.kompedent.png")
    style.
      .wf-loading * { visibility: hidden; }
      .wf-active * { visibility: visible; }
      .wf-loading { background-image: url('data:image/svg+xml;base64,'); background-repeat: no-repeat; background-position: center center; background-size: 50px; height: 100%; overflow: hidden; background-color: #004F9F; }
    link(rel="icon",href="favicon.ico",type="image/x-icon")
    link(rel="shortcut icon",href="favicon.ico",type="image/x-icon")
    link(rel="apple-touch-icon",href="public/images/icons/ios/AppIcon.appiconset/Icon-60@2x.png")
    link(rel="apple-touch-icon",sizes="180x180",href="public/images/icons/ios/AppIcon.appiconset/Icon-60@3x.png")
    link(rel="apple-touch-icon",sizes="76x76",href="public/images/icons/ios/AppIcon.appiconset/Icon-76.png")
    link(rel="apple-touch-icon",sizes="152x152",href="public/images/icons/ios/AppIcon.appiconset/Icon-76@2x.png")
    link(rel="apple-touch-icon",sizes="58x58",href="public/images/icons/ios/AppIcon.appiconset/Icon-Small@2x.png")
    link(rel="icon",sizes="192x192",href="public/images/icons/drawable-xxxhdpi/ic_launcher.png")
    // [if lt IE 9]
    script(src="//oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js",async)
    script(src="//oss.maxcdn.com/respond/1.4.2/respond.min.js",async)
    // [endif]
  body
    a(name="top",id="top")
    include _navigation.jade
    #content
      != yield
    if current.source != "index"
      .container
        footer
          .row
            .col-lg-9.col-lg-offset-3
              include _footer.jade
    else
      .container-full
        footer
          .row
            .col-lg-12
              include _footer.jade

    script(src="//ajax.googleapis.com/ajax/libs/jquery/2.1.4/jquery.min.js")
    script(src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.4/js/bootstrap.min.js",async)
    script(src="main.js", onload="init()")
    script.
      WebFontConfig = {
        google: {families: ['Source+Sans+Pro:100,200,400,700,700italic:latin']}
      };
      (function () {
        var wf = document.createElement('script');
        wf.src = ('https:' == document.location.protocol ? 'https' : 'http') + '://ajax.googleapis.com/ajax/libs/webfont/1/webfont.js';
        wf.type = 'text/javascript';
        wf.async = 'true';
        var s = document.getElementsByTagName('script')[0];
        s.parentNode.insertBefore(wf, s);
      })();
